[branch "master"]
  remote = origin
  merge = refs/heads/master
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[push]
	default = simple
[core]
	excludesfile = ~/.gitignore_global
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true

[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  pager = true

[gpg]
	program = gpg

